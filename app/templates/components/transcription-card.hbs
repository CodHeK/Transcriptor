  <div class="image">
    <img src="https://via.placeholder.com/150">
  </div>
  <div class="content">
    <div class="meta">
      <span> {{moment-format data.createdAt 'hh:mm a / DD-MM-YYYY'}}</span>
    </div>
    {{#link-to 'editor' class='header'}}{{data.name}}{{/link-to}}
    <div class="extra">
      {{#if data.audioDuration}}
        <div class="small text"><i class="wait icon"></i>{{data.audioDuration}}</div>
      {{/if}}
    </div>
  </div>
